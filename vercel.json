{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.1.5"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs20.x"
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ]
}
